                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginLeft="326dp"
                    android:layout_marginTop="720dp"
                    android:layout_marginEnd="25dp"
                    android:fontFamily="sans-serif"
                    android:gravity="center"
                    android:text="@string/__02_32_string"
                    android:textColor="@color/_01_35_color"
                    android:textSize="12sp" />

                <View
                    android:id="@+id/rectangle_1186"
                    android:layout_width="69dp"
                    android:layout_height="7dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="162dp"
                    android:layout_marginTop="42dp"
                    android:layout_marginEnd="162dp"
                    android:background="@drawable/rectangle_1186_shape" />

                <!--
                <Spinner
                    android:id="@+id/song_spinner"
                    android:layout_width="130dp"
                    android:layout_height="20dp"
                    android:background="@color/black"
                    android:gravity="center"
                    android:textColor="@color/white" />
                    -->

            </RelativeLayout>
